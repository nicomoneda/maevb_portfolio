<div class="container">
  <h1>Show Post</h1>
  <h1><%= @post.title %></h1>
  <p><%= @post.description %></p>
  <% @post.contents.each do |content| %>
    <%= image_tag content, alttext: "Image" %>
  <% end %>
  <%= link_to "Edit", edit_post_path(@post) %>
  <%= link_to "Delete", post_path(@post), method: :delete %>
  <%= link_to "Back", posts_path(category: @post.category) %>
</div>